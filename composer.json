{
  "name": "arduanov/silex-bootstrap",
  "description": "Silex bootstrap",
  "authors": [
    {
      "name": "Arduanov Marsel",
      "email": "arduanov@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "classmap": [
      "src/Lib/"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/arduanov/simple-record"
    }
  ],
  "scripts": {
    "post-root-package-install": [
      "./bin/install_db.sh",
      "php ./bin/db.console migrations:migrate"
    ]
  },
  "require": {
    "php": ">=5.6.0",
    "silex/silex": "~2.0@dev",
    "arduanov/simple-record": "dev-master",
    "doctrine/migrations": "1.0.*@dev",
    "doctrine/dbal": "~2.5",
    "predis/predis": "~1.0",
    "monolog/monolog": "~1.16",
    "symfony/config": "~2.7",
    "symfony/form": "~2.7",
    "symfony/validator": "~2.7",
    "doctrine/cache": "~1.4",
    "silex/web-profiler": "~2.0@dev",
    "texthtml/whoops-silex": "~1.0.3",
    "symfony/monolog-bridge": "~2.7",
    "symfony/asset": "~2.7",
    "symfony/security": "~2.7",
    "symfony/filesystem": "~2.7",
    "cocur/slugify": "~1.3"
  },
  "require-dev": {
    "codeception/codeception": "~2.1.0"
  }
}
